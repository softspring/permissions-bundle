{
    "name": "softspring/permissions-bundle",
    "description": "Symfony permissions management bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.9",
        "symfony/config": "^5.4|^6.0",
        "symfony/dependency-injection": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\PermissionsBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-5.1": "5.1.x-dev",
            "dev-5.2": "5.2.x-dev"
        }
    }
}
