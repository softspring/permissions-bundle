{
    "name": "softspring/permissions-bundle",
    "description": "Symfony permissions management bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.9",
        "symfony/config": "^5.4|^6.0|^7.0",
        "symfony/dependency-injection": "^5.4|^6.0|^7.0",
        "symfony/http-kernel": "^5.4|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\PermissionsBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
